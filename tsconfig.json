{
    "compilerOptions": {
      "target": "es5",
      "module": "commonjs",
      "lib": ["dom", "dom.iterable", "esnext"],
      "sourceMap": true,
      "esModuleInterop": true,
      "allowJs": true,
      "jsx": "react",
      "strict": true,
      "noImplicitAny": true,
      "moduleResolution": "node",
      "resolveJsonModule": true,
      "isolatedModules": true,
      "noEmit": true
    },
    "include": ["src","."]
  }
  